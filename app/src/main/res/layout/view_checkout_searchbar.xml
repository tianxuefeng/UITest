<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/txt_Stall_Name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/lbl_stallname"
        android:layout_weight="0.15"
        android:textSize="16sp" />

    <AutoCompleteTextView
        android:id="@+id/a_com_txt_view_checkout_type"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.7"
        android:background="@drawable/custom_edit_text"
        android:drawableRight="@drawable/abc_ic_clear_search_api_holo_light"
        android:hint="@string/msg_spinner_prompt_checkout_ch"
        android:longClickable="false"
        android:singleLine="true"
        android:textColor="@color/grey04" />

    <Button
        android:id="@+id/btn_authenticate"
        android:minWidth="30dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.15"
        android:backgroundTint="@color/blue01"
        android:text="Auth" />
</merge>
